{
  "name": "fpl-api-node",
  "version": "1.0.15",
  "description": "A simple node wrapper for the Fantasy Premier League (fantasy.premierleague.com) restful web api",
  "main": "dist/index.js",
  "typings": "dist/index",
  "files": [
    "dist/"
  ],
  "scripts": {
    "docs": "typedoc --out docs --name 'Fpl-api-node API' --mode file --readme none --mdHideIndexes --excludePrivate --theme markdown --mdOutFile API.md",
    "lint": "tslint --project tsconfig.json",
    "test": "mocha -r ts-node/register src/**/tests.ts --timeout 5000",
    "prepublish": "rm -rf dist && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tgreyuk/fpl-api-node.git"
  },
  "keywords": [
    "fantasy premier league",
    "fpl",
    "api"
  ],
  "author": "Thomas Grey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tgreyuk/fpl-api-node/issues"
  },
  "homepage": "https://github.com/tgreyuk/fpl-api-node#readme",
  "dependencies": {
    "axios": "^0.16.2",
    "memory-cache": "0.2.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/memory-cache": "0.0.29",
    "@types/mocha": "^2.2.41",
    "chai": "^4.1.0",
    "mocha": "^3.4.2",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "typedoc": "^0.7.1",
    "typedoc-plugin-markdown": "^1.0.2",
    "typescript": "2.4.1"
  }
}
